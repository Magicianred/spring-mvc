function buscar() {
	if($("#cep").val() != "") {
		$.ajax({
			url: "http://viacep.com.br/ws/" + $("#cep").val() + "/json/"
		}).done(function(e) {
			$("#rua").val(e.logradouro);
			$("#bairro").val(e.bairro);
			$("#cidade").val(e.cidade);
		console.log(e.cidade);
		}).fail(function(){
			alert("CEP n√£o encontrado!");
		});
	}
}